@model TergusWEB.Models.Cliente

@{
    ViewData["Title"] = "Editar Cliente Pessoa Jurídica";
}

<h1>Editar Cliente Pessoa Jurídica</h1>
<div class="form-group">
    <input type="submit" value="Save" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-danger">Voltar</a>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Nome_Razao_Social" class="control-label"></label>
                <input asp-for="Nome_Razao_Social" class="form-control" />
